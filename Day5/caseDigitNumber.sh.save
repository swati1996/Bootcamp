#!/bin/bash -x
read -p "Enter power of ten number : " num

case $nm in
	1 ) echo "Unit place"
	;;
	10 ) echo "Ten place"
	;;
	100 ) echo "Hundred place"
	;;
	1000 ) echo "Thousand place"
	;;
	10000 ) echo "Tens of thousand place"
	;;
	100000 ) echo "Hundred of thousand place"
	;;
	1000000 ) echo "Millions"

	;;
1 ) echo ""
	;;

